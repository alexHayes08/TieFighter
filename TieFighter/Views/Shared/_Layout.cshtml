<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width  initial-scale=1  shrink-to-fit=no">
    <title>@ViewData["Title"]</title>
    <link rel="icon" type="image/png" href="favicon.png">
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet"/>
    @*<link href="~/css/jquery.dataTables.css" rel="stylesheet" />*@
    <link href="https://cdn.datatables.net/1.10.16/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <environment names="Development">
        
    </environment>
    <environment names="Staging,Production">

    </environment>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/Loadable.css" />
    <link rel="stylesheet" href="~/css/site.css">
</head>
<body>
    <header>
        <nav class="navbar fixed-top navbar-expand-md navbar-dark">
            <a class="navbar-brand" href="#"><img class="img-fluid" src="~/images/Star_Wars_Yellow_Logo.svg" height="30">Tie Fighter - Beta</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="justify-content-end w-100 nav nav-pills">
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="enterGame" href="/Game/Index">Game</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="About">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Artwork">Artwork</a>
                    </li>
                    <li>
                        @await Html.PartialAsync("_LoginPartial")
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <main class="container-fluid">
        @RenderBody()
    </main>
    <footer></footer>
    <environment names="Development">
        <script src="https://apis.google.com/js/platform.js" async defer></script>
        <script src="~/lib/popper.js/dist/umd/popper.js"></script>
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/xhr.js"></script>
        <script src="~/js/loading.js"></script>
        <script src="~/js/common.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://apis.google.com/js/platform.js" async defer></script>
        <script src="~/lib/popper.js/dist/umd/popper.js"></script>
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/loading.js"></script>
        <script src="~/js/xhr.js"></script>
        <script src="~/js/common.js"></script>
    </environment>

    @*<script src="~/js/jquery.min.js"></script>
    <script src="~/js/popper.js"></script>
    <script src="~/js/bootstrap.min.js"></script>*@
    @*<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>*@
    @*<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
    <script src="~/js/xhr.js"></script>
    <script src="~/js/common.js"></script>
    @await  Html.PartialAsync("")*@
    @RenderSection("Scripts", required: false)
</body>
</html>